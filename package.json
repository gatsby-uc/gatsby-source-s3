{
  "name": "@robinmetral/gatsby-source-s3",
  "description": "A Gatsby plugin to source objects and images from AWS S3",
  "version": "0.0.0-semantically-released",
  "main": "gatsby-node.js",
  "author": "Robin MÃ©tral <robin@metral.ch>",
  "repository": "https://github.com/robinmetral/gatsby-source-s3.git",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "files": [
    "gatsby-node.js"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^14.0.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "gatsby": "^2.21.33",
    "husky": "^4.2.5",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.7",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "prestart": "yarn build && npm pack && (cd examples/gatsby-starter-source-s3 && yarn install && yarn add file:../../robinmetral-gatsby-source-s3-0.0.0-semantically-released.tgz)",
    "start": "(cd examples/gatsby-starter-source-s3 && gatsby build)"
  },
  "dependencies": {
    "aws-sdk": "^2.678.0",
    "gatsby-source-filesystem": "^2.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
